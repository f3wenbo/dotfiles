[core]
        quotepath = false
[user]
        email = wenbo.chen86@gmail.com
        name = Wenbo Chen
[alias]
        hist = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
        st = !"git stash list && git status --short --branch"
        cm = commit -m
        ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
        aa = !git add . && git add -u && git status
	    fpush = push
        blame = -w -C -C -C
[color]
        ui = auto
[credential]
        helper = cache
[push]
        default = simple
[pull]
        rebase = true
[init]
        defaultBranch = master
[rerere]
        enabled = true
        Update = true
[branch]
        sort = -committerdate
